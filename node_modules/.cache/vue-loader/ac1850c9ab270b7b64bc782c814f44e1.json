    </a-range-picker>\n      <!--查询按钮-->\n      <a-button type=\"primary\" icon=\"search\" @click=\"getneedtimelist(dateRange)\"> 查询 </a-button>\n    </div>\n\n    <a-table\n    :columns=\"columns\"\n    :data-source=\"data\"\n    bordered\n    size=\"middle\"\n    :scroll=\"{ x: 'calc(700px + 50%)', y: 240 }\"\n  />\n\n\n    <div>{{columns_date}}</div>\n\n  </div>\n</template>\n\n<script>\nimport moment from 'moment'\nimport 'moment/locale/zh-cn'\nimport { axios } from '@/utils/request'\n\nconst qadatas = {\n  statuscode: 200,\n  data: {\n    apartments: [{ id: 1, name: '商业QA' }],\n  },\n}\n\n\nconst columns = [\n  {title: 'name',\n   key :'name',\n   dataIndex: 'name'\n  },\n  {\n    title: '周一',\n    key:'2021-11-15',\n    dataIndex: '2021-11-15',\n    children : [\n      {title:'2021-11-15',dataIndex: '2021-11-15',key:'2021-11-15',}\n    ]\n  },\n  {\n    title: '周二',\n    key:'2021-11-16',\n    dataIndex: '2021-11-16',\n    children : [\n      {title:'2021-11-16',dataIndex: '2021-11-16',key:'2021-11-16'}\n    ]\n  },\n  {\n    title: '周三',\n    key:'2021-11-17',\n    dataIndex: '2021-11-17',\n    children : [\n      {title:'2021-11-17'}\n    ]\n  }\n]\n\nconst data = [\n  {\n    'name':'yu1',\n    '2021-11-15':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-16':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-17':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n  },\n  {\n    'name':'yu2',\n    '2021-11-15':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-16':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-17':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n  },\n  {\n    'name':'yu3',\n    '2021-11-15':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-16':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-17':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n  },\n\n]\n\nexport default {\n  name: 'qamemberdata',\n  data(){\n    return{\n      qadatas,\n      dateRange: [],\n      columns,\n      data,\n    }\n  },\n  methods:{\n    init(){\n       //周一 周日\n      const monday = this.dayjs().startOf('week').add(1, 'day')\n      const sunday = this.dayjs().endOf('week').add(1, 'day')\n\n      //时间转moment对象\n      this.dateRange = [\n        moment(monday.format('dddd MMM DD YYYY HH:mm:ss ZZ')),\n        moment(sunday.format('dddd MMM DD YYYY HH:mm:ss ZZ')),\n      ]\n\n      let temptime = [monday.format('YYYY-MM-DD HH:mm:ss'), sunday.format('YYYY-MM-DD HH:mm:ss')]\n      this.getqadate(temptime[0], temptime[1])\n    },\n\n    getdate(starttime, endtime){\n      \n    },\n    setapartmentid(apartmentid) {\n      this.apartmentid = apartmentid\n    },\n\n  },\n  computed:{\n    columns_date : function(){\n     \n      var start = this.dayjs().startOf('week').add(1, 'day').format('YYYY-MM-DD HH:mm:ss')\n      var stop  = this.dayjs().endOf('week').add(1, 'day').format('YYYY-MM-DD HH:mm:ss')\n      var array = new Array()\n      var current = new Date(start)\n      stop  = new Date(stop);\n      while (current <= stop) {\n          array.push( this.dayjs(new Date (current)).format('YYYY-MM-DD'))\n              current.setDate(current.getDate() + 1)\n        \n      }\n      return array.toString()\n        \n        \n      },\n      columns_week: function(){\n        return 222\n      },\n  }\n  \n}\n\n\n</script>\n\n<style scoped>\n.a-range-picker {\n  margin-left: 20px;\n  margin-right: 20px;\n}\n</style>"]}]}