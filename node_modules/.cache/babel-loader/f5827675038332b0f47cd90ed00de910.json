;AAAC;AACf,QAAIO,EAAE,CAACM,IAAH,KAAYT,cAAhB,EAAgC;AAC9BK,MAAAA,IAAI,CAAC;AAAEI,QAAAA,IAAI,EAAER;AAAR,OAAD,CAAJ;AACAX,MAAAA,SAAS,CAACoB,IAAV;AACD,KAHD,MAGO;AAEL;AAEH,UAAItB,KAAK,CAACuB,OAAN,CAAcC,KAAd,CAAoBC,MAApB,KAA+B,CAAnC,EAAsC;AACjCzB,QAAAA,KAAK,CAAC0B,QAAN,CAAe,SAAf,EAA0BC,IAA1B,CAA+B,UAAAC,GAAG,EAAI;AAAE;AACtC,cAAMJ,KAAK,GAAGI,GAAG,CAACC,WAAlB;AACA7B,UAAAA,KAAK,CAAC0B,QAAN,CAAe,gBAAf,EAAiC;AAAEF,YAAAA,KAAK,EAALA;AAAF,WAAjC,EAA4CG,IAA5C,CAAiD,YAAM;AACrD;AACA;AACA5B,YAAAA,MAAM,CAAC+B,SAAP,CAAiB9B,KAAK,CAACuB,OAAN,CAAcQ,UAA/B,EAHqD,CAIrD;;AACA,gBAAMC,QAAQ,GAAGC,kBAAkB,CAACjB,IAAI,CAACkB,KAAL,CAAWF,QAAX,IAAuBjB,EAAE,CAACM,IAA3B,CAAnC;;AACA,gBAAIN,EAAE,CAACM,IAAH,KAAYW,QAAhB,EAA0B;AACxB;AACAf,cAAAA,IAAI,iCAAMF,EAAN;AAAUoB,gBAAAA,OAAO,EAAE;AAAnB,iBAAJ;AACD,aAHD,MAGO;AACL;AACAlB,cAAAA,IAAI,CAAC;AAAEI,gBAAAA,IAAI,EAAEW;AAAR,eAAD,CAAJ;AACD;AACF,WAbD,EAFoC,CAgBpC;AACD,SAjBD,EAiBGI,KAjBH,CAiBS,UAACC,GAAD,EAAS;AAChBC,UAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;AACArC,UAAAA,KAAK,CAAC0B,QAAN,CAAe,WAAf,EAA4BC,IAA5B,CAAiC,YAAM;AACrCxB,YAAAA,YAAY,CAACqC,KAAb,CAAmB;AACjBC,cAAAA,OAAO,EAAE,IADQ;AAEjBC,cAAAA,WAAW,EAAE;AAFI,aAAnB,EADqC,CAKrC;;AACAzB,YAAAA,IAAI,CAAC;AAAEI,cAAAA,IAAI,EAAE;AAAR,aAAD,CAAJ;AACD,WAPD;AAQD,SA3BD;AA4BD,OA7BJ,MA6BU;AACLJ,QAAAA,IAAI;AACL;AAED;AACJ,GA1CA,MA0CM;AACJ,QAAIN,SAAS,CAACgC,QAAV,CAAmB5B,EAAE,CAAC6B,IAAtB,CAAJ,EAAiC;AAC/B;AACA3B,MAAAA,IAAI;AACL,KAHD,MAGO;AACLA,MAAAA,IAAI,CAAC;AAAEI,QAAAA,IAAI,EAAET,cAAR;AAAwBsB,QAAAA,KAAK,EAAE;AAAEF,UAAAA,QAAQ,EAAEjB,EAAE,CAAC8B;AAAf;AAA/B,OAAD,CAAJ;AACA3C,MAAAA,SAAS,CAACoB,IAAV,GAFK,CAEY;AAClB;AAEF;AACF,CAxDD;AA0DAvB,MAAM,CAAC+C,SAAP,CAAiB,YAAM;AACrB5C,EAAAA,SAAS,CAACoB,IAAV,GADqB,CACJ;AAClB,CAFD","sourcesContent":["import router from './router'\nimport store from './store'\nimport storage from 'store'\nimport NProgress from 'nprogress' // progress bar\nimport '@/components/NProgress/nprogress.less' // progress bar custom style\nimport notification from 'ant-design-vue/es/notification'\nimport { setDocumentTitle, domTitle } from '@/utils/domUtil'\nimport { ACCESS_TOKEN } from '@/store/mutation-types'\nimport { i18nRender } from '@/locales'\nimport { getToken } from './utils/auth'\n\nNProgress.configure({ showSpinner: false }) // NProgress Configuration\n\nconst whiteList = ['login', 'register', 'registerResult'] // no redirect whitelist\nconst loginRoutePath = '/user/login'\nconst defaultRoutePath = '/dashboard/analysis'\n\nrouter.beforeEach((to, from, next) => {\n  NProgress.start() // start progress bar\n  to.meta && (typeof to.meta.title !== 'undefined' && setDocumentTitle(`${i18nRender(to.meta.title)} - ${domTitle}`))\n  /* has token */\n  if (getToken()) {//storage.get(ACCESS_TOKEN)\n    if (to.path === loginRoutePath) {\n      next({ path: defaultRoutePath })\n      NProgress.done()\n    } else {\n\n      //check login user.roles is null\n\n   if (store.getters.roles.length === 0) {\n        store.dispatch('GetInfo').then(res => { // 拉取用户信息\n          const roles = res.permissions\n          store.dispatch('GenerateRoutes', { roles }).then(() => {\n            // 根据roles权限生成可访问的路由表\n            // 动态添加可访问路由表\n            router.addRoutes(store.getters.addRouters)\n            // 请求带有 redirect 重定向时，登录自动重定向到该地址\n            const redirect = decodeURIComponent(from.query.redirect || to.path)\n            if (to.path === redirect) {\n              // set the replace: true so the navigation will not leave a history record\n              next({ ...to, replace: true })\n            } else {\n              // 跳转到目的路由\n              next({ path: redirect })\n            }\n          })\n          //next({path: defaultRoutePath})\n        }).catch((err) => {\n          console.log(err)\n          store.dispatch('FedLogOut').then(() => {\n            notification.error({\n              message: '错误',\n              description: '请求用户信息失败，请重试'\n            })\n            //Message.error(err || 'Verification failed, please login again')\n            next({ path: '/' })\n          })\n        })\n      } else {\n        next()\n      }\n\n     }\n } else {\n    if (whiteList.includes(to.name)) {\n      // 在免登录白名单，直接进入\n      next()\n    } else {\n      next({ path: loginRoutePath, query: { redirect: to.fullPath } })\n      NProgress.done() // if current page is login will not trigger afterEach hook, so manually handle it\n    }\n\n  }\n})\n\nrouter.afterEach(() => {\n  NProgress.done() // finish progress bar\n})\n"]}]}