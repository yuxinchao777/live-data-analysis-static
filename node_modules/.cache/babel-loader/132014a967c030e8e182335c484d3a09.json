A,IAAA,CAAA,KAAA,CAAA;AACA,MAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;;AACA,aAAA,OAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA;AAAA,mBAAA,KAAA,QAAA,CAAA,KAAA,KAAA,CAAA,IAAA,IAAA,CAAA,OAAA,CAAA,EAAA,GAAA,EAAA,CAAA;AACA,sBAAA,CAAA;AAAA,qBAAA,KAAA,KAAA,CAAA,IAAA,IAAA,CAAA,OAAA,CAAA,EAAA,MAAA,CAAA,YAAA,CAAA;AACA,mBAAA,KAAA,KAAA,CAAA,IAAA,IAAA,CAAA,OAAA,CAAA,EAAA,MAAA,CAAA,YAAA,CADA;AAEA,yBAAA,KAAA,KAAA,CAAA,IAAA,IAAA,CAAA,OAAA,CAAA,EAAA,MAAA,CAAA,YAAA,CAFA;AAGA,qBAAA,EAHA;AAIA,YAAA,WAAA,EAAA;AAAA,cAAA,YAAA,EAAA;AAAA;AAJA,WAAA;AADA,SAAA;AASA,QAAA,OAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA,KAAA,CAAA;AACA;;AACA,WAAA,gBAAA,GAAA,KAAA;AACA,KApEA;AAuEA,IAAA,QAvEA,oBAuEA,GAvEA,EAuEA;AACA,UAAA,KAAA,GAAA,IAAA,KAAA,CAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,CAAA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,GAAA,CAAA;AACA,aAAA,IAAA;AACA;AA3EA,GAZA;AA0FA,EAAA,QAAA,EAAA,EA1FA;AA6FA,EAAA,OA7FA,qBA6FA;AACA,SAAA,IAAA;AACA;AA/FA,CAAA","sourcesContent":["<template>\n  <div>\n    <div>\n      <!--部门下拉框-->\n      部门<a-select default-value=\"商业QA\" style=\"width: 100px; margin-left: 5px\">\n        <a-select-option\n          v-for=\"apartment in qadatas.data.apartments\"\n          :key=\"apartment.id\"\n          :value=\"apartment.name\"\n          @click=\"setapartmentid(apartment.id)\"\n        >\n          {{ apartment.name }}\n        </a-select-option>\n      </a-select>\n      <!--时间选择框-->\n      <a-range-picker class=\"a-range-picker\" style=\"width: 400px\" show-time v-model=\"dateRange\">\n        <template slot=\"renderExtraFooter\"> 日期选择</template>\n      </a-range-picker>\n      <!--查询按钮-->\n      <a-button type=\"primary\" icon=\"search\" @click=\"getneedtimelist(dateRange)\" @change=\"getneedtimelist(dateRange)\"> 查询 </a-button>\n    </div>\n\n    <a-table\n    :columns=\"all_columns_data\"\n    :data-source=\"data\"\n    style=\"margin-top: 20px;\"\n    :pagination=\"{ defaultPageSize: 100 }\"\n  >\n    <div slot=\"action\" slot-scope=\"text\">\n\n      <div>\n      <!--自定义内容-->\n      <div v-if='text.qadata !==\"休息日\" && text.qadata !==\"请假\" '>\n        <a-input  v-for=\"(item,index) in text.qadata\" :key=index :defaultValue=item >\n           {{item}}\n        </a-input>\n        <div>{{text111}}</div>\n        <a-textarea :rows=\"4\" :value='text111' disabled></a-textarea>\n      </div>\n      <div v-else-if=' text.qadata ===\"休息日\" '>\n        <a-input  defaultValue='休息日' />{{item}}\n      </div>\n      <div v-else-if=' text.qadata ===\"请假\" '>\n        <a-input  defaultValue='请假' />{{item}}\n      </div>\n      <a-divider dashed=true />\n\n      <!--tb内容-->\n      <div v-if='text.tbdata !==\"休息日\" && text.tbdata !==\"请假\" '>\n        <a  v-for=\"(item,index) in text.tbdata\" :key=index :defaultValue=item />{{item}}\n      </div>\n      <div v-else-if=' text.tadata ===\"休息日\" '>\n        <a-input  defaultValue='休息日' />{{item}}\n      </div>\n      <div v-else-if=' text.tbdata ===\"请假\" '>\n        <a-input  defaultValue='请假' />{{item}}\n      </div>\n      </div>\n    </div>\n    \n\n  </a-table>\n\n\n  </div>\n</template>\n\n<script>\nimport moment from 'moment'\nimport 'moment/locale/zh-cn'\nimport { axios } from '@/utils/request'\n\nconst qadatas = {\n  statuscode: 200,\n  data: {\n    apartments: [{ id: 1, name: '商业QA' }],\n  },\n}\n\n\nconst data = [\n  {\n    'name':'yu1',\n    'apartmentname':'zengzhi',\n    '2021-11-29':{\n                },\n    '2021-11-30':{\n                'qadata':['111','111','111'],\n                'tbdata':['tb111','tb111','tb111']},\n    '2021-12-01':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-12-02':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n  },\n  {\n    'name':'yu2',\n    'apartmentname':'zengzhi',\n    '2021-11-29':{\n                'qadata':['222','222','222'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-30':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-12-01':{\n                'qadata':['111','222','333'],\n                'tbdata':['tb111','tb222','tb333']},\n  },\n  {\n    'name':'yu3',\n    'apartmentname':'zengzhi',\n    '2021-11-29':{\n                'qadata':['333','333','333'],\n                'tbdata':['tb111','tb222','tb333']},\n    '2021-11-30':{\n                'qadata':\"休息日\",\n                'tbdata':[]},\n    '2021-12-06':{\n                'qadata':['111','222','333456'],\n                'tbdata':['tb111','tb222','tb333567']},\n  },\n\n]\n\nconst text111 = \"<a href='https://www.baidu.com'> baidu </a>\"\n\nexport default {\n  name: 'qamemberdata',\n  data(){\n    return{\n      qadatas,\n      dateRange: [],\n      data,\n      all_columns_data:[],\n      text111\n    }\n  },\n\n  methods:{\n    init(){\n       //周一 周日\n      const monday = this.dayjs().startOf('week').add(1, 'day')\n      const sunday = this.dayjs().endOf('week').add(1, 'day')\n\n      //时间转moment对象\n      this.dateRange = [\n        moment(monday.format('dddd MMM DD YYYY HH:mm:ss ZZ')),\n        moment(sunday.format('dddd MMM DD YYYY HH:mm:ss ZZ')),\n      ]\n\n      this.get_columns(this.dateRange)\n      let temptime = [monday.format('YYYY-MM-DD HH:mm:ss'), sunday.format('YYYY-MM-DD HH:mm:ss')]\n      this.getdate(temptime[0], temptime[1])\n    },\n\n    //获取列表数据\n    getdate(starttime, endtime){\n      this.get_columns(this.dateRange)\n    },\n\n    setapartmentid(apartmentid) {\n      this.apartmentid = apartmentid\n    },\n\n    //点击查询获取列表数据\n    getneedtimelist(){\n      this.get_columns(this.dateRange)\n  \n    },\n\n\n\n    //获取表头\n    get_columns(dateRange){\n      var start = this.dayjs(dateRange[0].valueOf()).format('YYYY-MM-DD HH:mm:ss')\n      var stop = this.dayjs(dateRange[1].valueOf()).format('YYYY-MM-DD HH:mm:ss')\n      var array = new Array()\n\n      //填下业务线\n      array.push({'title': '业务线',\n                  'key' :'apartmentname',\n                  'dataIndex': 'apartmentname',\n                  'width':50,\n      })\n\n      //添加姓名\n      array.push({'title': '姓名',\n                  'key' :'name',\n                  'dataIndex': 'name',\n                  'width':50,\n      })\n      var current = new Date(start)\n      stop  = new Date(stop);\n      while (current <= stop) {\n          array.push( {'title':this.get_week( this.dayjs(new Date (current)).day() ),\n                      'children':[{'title':this.dayjs(new Date (current)).format('YYYY-MM-DD'),\n                                  'key':this.dayjs(new Date (current)).format('YYYY-MM-DD'),\n                                  'dataIndex':this.dayjs(new Date (current)).format('YYYY-MM-DD'),\n                                  'width':50,\n                                  scopedSlots: { customRender: 'action' },\n                                  }]\n                      })\n\n              current.setDate(current.getDate() + 1)\n      }\n      this.all_columns_data = array\n    },\n\n\n    get_week(num){\n      var weeks = new Array(\"星期日\", \"星期一\", \"星期二\", \"星期三\", \"星期四\", \"星期五\", \"星期六\")\n      var week = weeks[num]\n      return week\n    },\n\n  },\n  computed:{\n    \n  },\n  created() {\n    this.init()\n  },\n  \n}\n\n\n</script>\n\n<style scoped>\n.a-range-picker {\n  margin-left: 20px;\n  margin-right: 20px;\n}\n.ant-table td { \n  white-space: nowrap; \n}\n</style>"],"sourceRoot":"src/views/qadata"}]}