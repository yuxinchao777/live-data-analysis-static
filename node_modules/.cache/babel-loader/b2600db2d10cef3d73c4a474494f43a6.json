SA,GAXA;AAaA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,aAAA;AACA,KAHA;AAIA,IAAA,aAJA,2BAIA;AAAA;;AACA,WAAA,SAAA,GAAA,IAAA;AACA,MAAA,KAAA,CACA,IADA,CACA,gBADA,EACA;AACA,QAAA,UAAA,EAAA;AADA,OADA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,UAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,gBAAA,IAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,cAAA,IAAA,CAAA,MAAA,GAAA,GAAA;AAAA,aADA,MAEA;AACA,cAAA,IAAA,CAAA,MAAA,GAAA,GAAA;AACA;AACA,WANA;AAOA,UAAA,KAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA;AACA,OAhBA,EAiBA,KAjBA,CAiBA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,OApBA;AAqBA,KA3BA;AA6BA,IAAA,SA7BA,qBA6BA,EA7BA,EA6BA,IA7BA,EA6BA,MA7BA,EA6BA,KA7BA,EA6BA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,UAAA,MAAA,KAAA,GAAA,EAAA;AACA,aAAA,WAAA,GAAA,IAAA;AACA,OAFA,MAGA;AACA,aAAA,WAAA,GAAA,KAAA;AACA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KAzCA;AA0CA,IAAA,QA1CA,sBA0CA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CACA,UADA,EACA;AACA,QAAA,EAAA,EAAA,KAAA,OADA;AAEA,QAAA,OAAA,EAAA,KAAA,UAFA;AAGA,QAAA,MAAA,EAAA,KAAA;AAHA,OADA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAJA,MAKA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,OAnBA;AAoBA,KA/DA;AAgEA,IAAA,QAhEA,oBAgEA,OAhEA,EAgEA;AACA,MAAA,WAAA,GAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,uBAAA,OAAA;AACA;AAnEA,GAbA;AAoFA,EAAA,QAAA,EAAA,EApFA;AAqFA,EAAA,OArFA,qBAqFA;AACA,SAAA,IAAA;AACA;AAvFA,CAAA","sourcesContent":["<template>\n    <div>\n        <h1>qa人员配置</h1>\n        <a-table\n      :columns=\"columns\"\n      :data-source=\"qadata\"\n      style=\"margin-top: 20px;\"\n      :loading=\"isLoading\"\n      :pagination=\"{ defaultPageSize: 100 }\"\n    >\n      <div slot=\"action\" slot-scope=\"text, record\">\n        <div>\n            <a-button type=\"primary\" @click=\"showModal(record.id,record.name,record.isshow,record.orderId)\">编辑</a-button>\n        </div>\n      </div>\n    </a-table>\n    <a-modal v-model=\"visible\" title=\"编辑\" @ok=\"handleOk\" >\n                <p>姓名: {{modelname}}</p>\n                <p>权重: <a-input-number\n                        placeholder=\"权重\" \n                        style=\"width:80px;\" \n                        v-model=modelorder\n                        /> </p>\n                <p>显示: <a-switch v-model=modelisshow @change=\"onChange\"  /></p>\n    </a-modal>\n    </div>\n</template>\n\n<script>\nimport { axios } from '@/utils/request'\nimport { method } from 'lodash'\n\nconst columns = [\n  { title: 'id', width: 30, dataIndex: 'id', key: 'id' },\n  { title: '姓名', width: 30, dataIndex: 'name', key: 'name' },\n  { title: '权重', width: 30, dataIndex: 'orderId', key: 'orderId' },\n  { title: '列表显示', width: 30, dataIndex: 'isshow', key: 'isshow' },\n  {\n    title: '操作',\n    key: 'taskDetail',\n    width: 250,\n    scopedSlots: { customRender: 'action' },\n  },\n]\n\n\nexport default {\n  data() {\n    return {\n      columns,\n      qadata:[],\n      visible: false,\n      modelid:\"\",\n      modelorder:\"\",\n      modelname:\"\",\n      modelisshow:true\n    }\n  },\n  \n  methods : {\n    init(){\n        this.getmemberdata()\n    },\n    getmemberdata(){\n        this.isLoading = true\n        axios\n        .post('/queryEmployee', {\n          Department: 'qa',\n        })\n        .then((response) => {\n          if (response.code === 200) {\n            this.isLoading = false\n            response.data.forEach(item=>{\n                if (item.isshow===1){\n                item.isshow = \"是\"}\n                else{\n                    item.isshow = \"否\"\n                }\n                })\n            this.qadata = response.data\n          }\n        })\n        .catch((error) => {\n          this.isLoading = false\n          this.$message.error('内网数据获取失败')\n        })\n    },\n\n    showModal(id,name,isshow,order) {\n        this.modelid = id\n        this.modelname = name\n        this.modelorder=order\n        console.log(order)\n        if(isshow === \"是\"){\n            this.modelisshow=true\n        }\n        else{\n            this.modelisshow=false\n        }\n        this.visible = true;\n    },\n    handleOk() {\n        axios\n        .post('/qaorder', {\n          id : this.modelid,\n          orderId : this.modelorder,\n          isshow : this.modelisshow\n        })\n        .then((response) => {\n          if (response.code === 200) {\n              this.$message.success('提交成功')\n            this.visible = false;\n            this.init()\n          }\n          else{\n              this.$message.error('提交失败')\n          }\n        })\n        .catch((error) => {\n          this.isLoading = false\n          this.$message.error('内网数据获取失败')\n        })\n    },\n    onChange(checked) {\n        modelisshow = checked\n        console.log(`a-switch to ${checked}`);\n    },\n    \n\n  },\n  computed: {},\n  created() {\n    this.init()\n  },\n}\n</script>\n\n<style scoped>\n\n\n</style>\n"],"sourceRoot":"src/views/qadata"}]}